{
  "manifest_version": 3,
  "name": "Map.ly",
  "version": "1.1.0",
  "description": "Link Management Extension. Map keys to URLs.",
  "permissions": [
    "storage",
    "tabs",
    "bookmarks"
  ],
  "omnibox": {
    "keyword": "mp"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "icons": {
    "16": "assets/icon/icon16.png",
    "48": "assets/icon/icon48.png",
    "128": "assets/icon/icon128.png"
  }
}